start : expr
      | list

list : "(" start ")"
     | "(" start list ")"

expr : SIGNED_NUMBER
     | ESCAPED_STRING
     | CNAME
     | comment
     | MATH_OP

operator : MATH_OP
         | CNAME

comment : "!@" ANY "@!"

ANY : /.+/
MATH_OP : /\+|\-|\/|\*/

%import common.ESCAPED_STRING
%import common.SIGNED_NUMBER
%import common.CNAME
%import common.WS
%ignore WS
